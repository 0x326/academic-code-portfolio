# EditorConfig: https://EditorConfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space

[*.html]
indent_size = 2

[*.js]
indent_size = 2

[*.css]
indent_size = 4

[{*.json,*.yml}]
indent_size = 2

[Makefile]
indent_style = tab

[*.php]
indent_size = 4
